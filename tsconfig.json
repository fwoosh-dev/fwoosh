{
  "compilerOptions": {
    "declaration": true,
    "importHelpers": true,
    "incremental": true,
    "allowJs": true,
    "module": "ES2020",
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "outDir": "lib",
    "strict": true,
    "jsx": "react-jsx",
    "target": "ES2018",
    "lib": ["es2019", "DOM"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
