{
  "name": "fwoosh-monorepo",
  "version": "0.0.14",
  "description": "A lightening quick MDX static website generator!",
  "workspaces": [
    "packages/*",
    "plugins/*",
    "examples/*"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.dev.json",
    "start": "npm run build -- --watch",
    "release": "auto shipit"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "auto": "^10.36.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "fwoosh": "link:./packages/cli"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
