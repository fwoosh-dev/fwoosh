{
  "name": "fwoosh",
  "type": "module",
  "version": "0.0.1",
  "description": "A lightening quick MDX static website generator",
  "main": "lib/index.js",
  "bin": "bin/fwoosh.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build --watch",
    "package": "rm -rf binary && pkg . --out-path binary --targets node14-macos-x64",
    "fwoosh": "chmod +x ./bin/fwoosh.js && ./bin/fwoosh.js"
  },
  "keywords": [
    "documentation",
    "static",
    "generator"
  ],
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@jesec/pkg": "^4.4.9",
    "@types/find-cache-dir": "^3.2.0",
    "@types/fs-extra": "^9.0.7",
    "@types/live-server": "^1.2.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "fwoosh": "link:./",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2"
  }
}
