{
  "name": "fwoosh",
  "type": "module",
  "version": "0.0.14",
  "description": "A lightening quick MDX static website generator!",
  "main": "lib/index.js",
  "bin": "bin/fwoosh.js",
  "scripts": {
    "build": "tsc -b tsconfig.dev.json",
    "start": "npm run build -- --watch",
    "vercel-build": "yarn build && yarn fwoosh build",
    "release": "auto shipit"
  },
  "devDependencies": {
    "@types/find-cache-dir": "^3.2.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "auto": "^10.36.5",
    "prettier": "^2.2.1",
    "typescript": "^4.2.2",
    "wsrun": "^5.2.4"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "await-to-js": "^2.1.1",
    "clsx": "^1.1.1",
    "command-line-application": "^0.10.1",
    "cosmiconfig": "^7.0.0",
    "execa": "^5.0.0",
    "find-cache-dir": "^3.3.1",
    "fwoosh": "link:./packages/cli",
    "open": "^7.4.2",
    "ora": "^5.3.0",
    "pretty-ms": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tapable": "^2.2.0"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
