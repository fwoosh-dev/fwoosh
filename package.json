{
  "name": "fwoosh-monorepo",
  "private": true,
  "version": "0.0.15",
  "description": "A lightening quick MDX static website generator!",
  "author": "Andrew Lisowski <lisowski54@gmail.com>",
  "workspaces": [
    "packages/*",
    "plugins/*",
    "examples/*"
  ],
  "scripts": {
    "clean": "rm -rf **/tsconfig.tsbuildinfo && rm -rf **/dist && rm -rf **/node_modules",
    "build": "tsc -b tsconfig.dev.json",
    "start": "npm run build -- --watch",
    "release": "auto shipit"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "auto": "^10.37.6",
    "patch-package": "^6.4.7",
    "prettier": "^2.2.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "fwoosh": "link:./packages/cli"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "packageManager": "yarn@3.2.0",
  "resolutions": {
    "@devtools-ds/tree@1.1.2": "patch:@devtools-ds/tree@npm:1.1.2#.yarn/patches/@devtools-ds-tree-npm-1.1.2-f6d01efdfa.patch",
    "@devtools-ds/object-inspector@1.1.2": "patch:@devtools-ds/object-inspector@npm:1.1.2#.yarn/patches/@devtools-ds-object-inspector-npm-1.1.2-2e6f63de45.patch"
  }
}
